{
  "name": "podo-ui",
  "version": "1.0.4",
  "type": "module",
  "author": "hada0127 <work@tarucy.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hada0127/podo-ui.git"
  },
  "bugs": {
    "url": "https://github.com/hada0127/podo-ui/issues"
  },
  "homepage": "https://podoui.com",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "cdn",
    "vanilla",
    "scss",
    "global.scss",
    "global.d.ts",
    "mixin.scss",
    "vite-fonts.scss",
    "README.md",
    "README.ko.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react/atom/*": {
      "types": "./dist/react/atom/*.d.ts",
      "import": "./dist/react/atom/*.js",
      "default": "./dist/react/atom/*.js"
    },
    "./react/molecule/*": {
      "types": "./dist/react/molecule/*.d.ts",
      "import": "./dist/react/molecule/*.js",
      "default": "./dist/react/molecule/*.js"
    },
    "./svelte": {
      "types": "./dist/svelte/index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "import": "./dist/svelte/index.js",
      "default": "./dist/svelte/index.js"
    },
    "./svelte/atom/*": {
      "types": "./dist/svelte/atom/*.d.ts",
      "svelte": "./dist/svelte/atom/*.svelte",
      "import": "./dist/svelte/atom/*.js",
      "default": "./dist/svelte/atom/*.js"
    },
    "./svelte/molecule/*": {
      "types": "./dist/svelte/molecule/*.d.ts",
      "svelte": "./dist/svelte/molecule/*.svelte",
      "import": "./dist/svelte/molecule/*.js",
      "default": "./dist/svelte/molecule/*.js"
    },
    "./global.scss": {
      "types": "./global.d.ts",
      "default": "./global.scss"
    },
    "./mixin.scss": {
      "types": "./global.d.ts",
      "default": "./mixin.scss"
    },
    "./vite-fonts.scss": {
      "types": "./global.d.ts",
      "default": "./vite-fonts.scss"
    },
    "./scss/*": {
      "types": "./global.d.ts",
      "default": "./scss/*"
    },
    "./package.json": "./package.json",
    "./cdn/podo-ui.css": "./cdn/podo-ui.css",
    "./cdn/podo-ui.min.css": "./cdn/podo-ui.min.css",
    "./cdn/podo-datepicker.js": "./cdn/podo-datepicker.js",
    "./cdn/podo-datepicker.min.js": "./cdn/podo-datepicker.min.js",
    "./cdn/podo-datepicker.css": "./cdn/podo-datepicker.css",
    "./cdn/podo-datepicker.min.css": "./cdn/podo-datepicker.min.css",
    "./cdn/font/*": "./cdn/font/*",
    "./vanilla/datepicker": "./vanilla/datepicker.js",
    "./vanilla/datepicker.css": "./vanilla/datepicker.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vike prerender",
    "preview": "vite preview",
    "build:lib": "node ./cli/build-lib.js",
    "build:cdn": "node ./cli/build-cdn.js",
    "build:cdn-js": "node ./cli/build-cdn-js.js",
    "build:svelte": "node ./cli/build-svelte.js",
    "build:all": "npm run build:lib && npm run build:svelte && npm run build:cdn && npm run build:cdn-js",
    "prepublishOnly": "npm run build:all",
    "lint": "eslint .",
    "icon": "node ./cli/icon-scss.js",
    "deploy": "vite build && wrangler pages deploy dist/client",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "sass": "^1.0.0",
    "svelte": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "sass": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "dependencies": {
    "shiki": "^3.19.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@playwright/test": "^1.56.1",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "22.10.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "glob": "^11.0.3",
    "globals": "^15.9.0",
    "i18next": "^24.0.0",
    "jsdom": "^25.0.1",
    "opentype.js": "^1.3.4",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "sass": "^1.83.0",
    "sass-embedded": "^1.77.8",
    "scss": "^0.2.4",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vike": "^0.4.182",
    "vike-react": "^0.4.18",
    "vite": "^5.4.0",
    "vitest": "^2.1.8"
  }
}
